{
  "name": "gsmeira/laravel-attachments",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.52 || ^10.0",
    "illuminate/database": "^9.52 || ^10.0",
    "illuminate/filesystem": "^9.52 || ^10.0",
    "illuminate/http": "^9.52 || ^10.0",
    "illuminate/validation": "^9.52 || ^10.0"
  },
  "autoload": {
    "psr-4": {
      "GSMeira\\LaravelAttachments\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "GSMeira\\LaravelAttachments\\AttachmentsServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.10",
    "pestphp/pest": "^2.18"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
